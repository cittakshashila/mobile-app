{
  "expo": {
    "name": "cittk",
    "slug": "cittk",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "cittk",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cittk.cittk"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.cittk.cittk"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "b8b8e114-1649-4fd8-8251-15218e3b6029"
      }
    },
    "owner": "cittk",
    "android": {
      "package": "com.cittk.cittk",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "cittk",
              "host": "*",
              "pathPrefix": "/",
              "autoVerify": true
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "ios": {
      "bundleIdentifier": "com.cittk.cittk",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleTypeRole": "Editor",
            "CFBundleURLSchemes": ["cittk"]
          }
        ]
      }
    }
  }
}
