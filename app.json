{
  "expo": {
    "name": "cittk",
    "slug": "cittk",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "cittk",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
        "config": {
            "usesNonExemptEncryption": false
        },
      "bundleIdentifier": "com.cittk.cittk",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleTypeRole": "Editor",
            "CFBundleURLSchemes": [
              "cittk"
            ]
          }
        ]
      }
    },
    "android": {
      "package": "com.cittk.cittk",
      "intentFilters": [
        {
          "autoVerify": true,
          "action": "VIEW",
          "data": [
            {
              "scheme": "cittk"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "b8b8e114-1649-4fd8-8251-15218e3b6029"
      }
    },
    "owner": "cittk",
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/b8b8e114-1649-4fd8-8251-15218e3b6029"
    },
    "plugins": [
      "expo-secure-store"
    ]
  }
}
