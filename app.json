{
  "expo": {
    "name": "events",
    "slug": "events",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.cittk.events"
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://evanbacon.dev"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to create event photos"
        }
      ], [
        "expo-barcode-scanner",
        {
          "cameraPermission": "Allow this app to access camera."
        }
      ], [
        "expo-camera",
        {
          "cameraPermission": "Allow this app to access your camera."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": "https://evanbacon.dev"
      },
      "eas": {
        "projectId": "428d6962-d5f8-48bb-a2fd-894e6caac68b"
      }
    }
  }
}
